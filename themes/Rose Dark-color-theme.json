{
    "name": "Rose Dark",
    "type": "dark",
    "colors": {
        "editor.background": "#1e1a1d",
        "tab.activeBackground": "#1e1a1d",
        "tab.inactiveBackground": "#171416",
        "tab.activeForeground": "#ffd6e0",
        "tab.inactiveForeground": "#a68a93",
        "editor.foreground": "#ffe0e9",
        "editor.selectionBackground": "#4a3c42",
        "editor.lineHighlightBackground": "#292326",
        "editorCursor.foreground": "#ffa6c1",
        "editorLineNumber.foreground": "#635259",
        "editorLineNumber.activeForeground": "#ffa6c1",
        "activityBar.background": "#141113",
        "activityBar.foreground": "#ffa6c1",
        "activityBarBadge.background": "#ff8fab",
        "activityBarBadge.foreground": "#1e1a1d",
        "sideBar.background": "#171416",
        "sideBar.foreground": "#d1b3be",
        "sideBarTitle.foreground": "#ffa6c1",
        "statusBar.background": "#141113",
        "statusBar.foreground": "#ffa6c1",
        "titleBar.activeBackground": "#141113",
        "titleBar.activeForeground": "#ffa6c1"
    },
    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#856f79"
            }
        },
        {
            "name": "Keyword",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#ff92b2",
                "fontStyle": "bold"
            }
        },
        {
            "name": "String",
            "scope": [
                "string",
                "constant.other.symbol",
                "constant.other.key"
            ],
            "settings": {
                "foreground": "#ffccd5"
            }
        },
        {
            "name": "Function",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "support.function"
            ],
            "settings": {
                "foreground": "#ffacc5",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable",
                "string constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#ffe0e9"
            }
        },
        {
            "name": "Number",
            "scope": [
                "constant.numeric",
                "constant.language"
            ],
            "settings": {
                "foreground": "#ffb7c5"
            }
        },
        {
            "name": "Type",
            "scope": [
                "entity.name.type",
                "support.class",
                "support.type"
            ],
            "settings": {
                "foreground": "#ffc2d1",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator",
                "punctuation"
            ],
            "settings": {
                "foreground": "#ff9eb5"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#ff6b6b"
            }
        }
    ]
}