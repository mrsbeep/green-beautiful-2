{
    "name": "Lavender Dark",
    "type": "dark",
    "colors": {
        "editor.background": "#1c1a1f",
        "tab.activeBackground": "#1c1a1f",
        "tab.inactiveBackground": "#161418",
        "tab.activeForeground": "#f0e6ff",
        "tab.inactiveForeground": "#958da1",
        "editor.foreground": "#f0e6ff",
        "editor.selectionBackground": "#3e3845",
        "editor.lineHighlightBackground": "#252229",
        "editorCursor.foreground": "#dca6ff",
        "editorLineNumber.foreground": "#5e5766",
        "editorLineNumber.activeForeground": "#dca6ff",
        "activityBar.background": "#131114",
        "activityBar.foreground": "#dca6ff",
        "activityBarBadge.background": "#c6b0ff",
        "activityBarBadge.foreground": "#1c1a1f",
        "sideBar.background": "#161418",
        "sideBar.foreground": "#c4bed1",
        "sideBarTitle.foreground": "#dca6ff",
        "statusBar.background": "#131114",
        "statusBar.foreground": "#dca6ff",
        "titleBar.activeBackground": "#131114",
        "titleBar.activeForeground": "#dca6ff"
    },
    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#7d758a"
            }
        },
        {
            "name": "Keyword",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#dca6ff",
                "fontStyle": "bold"
            }
        },
        {
            "name": "String",
            "scope": [
                "string",
                "constant.other.symbol",
                "constant.other.key"
            ],
            "settings": {
                "foreground": "#ebd6ff"
            }
        },
        {
            "name": "Function",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "support.function"
            ],
            "settings": {
                "foreground": "#c6b0ff",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable",
                "string constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#f0e6ff"
            }
        },
        {
            "name": "Number",
            "scope": [
                "constant.numeric",
                "constant.language"
            ],
            "settings": {
                "foreground": "#e2b8ff"
            }
        },
        {
            "name": "Type",
            "scope": [
                "entity.name.type",
                "support.class",
                "support.type"
            ],
            "settings": {
                "foreground": "#d4b3ff",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator",
                "punctuation"
            ],
            "settings": {
                "foreground": "#dca6ff"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#ff6b6b"
            }
        }
    ]
}